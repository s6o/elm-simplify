{
  "name": "elm_simplify_demo",
  "main": "../elm.js",
  "version": "1.0.0",
  "dependencies": {
  },
  "private": true,
  "scripts": {
    "watch-elm"  : "chokidar './*.elm' -c 'elm-make ./Main.elm --output ./../elm.js' --initial",
    "watch" : "npm run watch-elm",
    "build-elm": "elm-make ./Main.elm --output ./../elm.js && uglifyjs ./../elm.js --screw-ie8 -c -m -o ./../elm.min.js",
    "build": "npm run build-elm"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "uglify-js" : "2.7.5",
    "watchify" : "3.7.0",
    "browserify" : "13.0.1",
    "watch-run" : "1.2.4",
    "babelify" : "7.3.0",
    "babel-preset-es2015": "6.18.0",
    "elm": "0.18.0"
  },
  "browserify": {
    "transform": [
      ["babelify", {"presets": [["es2015", {"loose":true}], "react"]}]
    ]
  }
}
